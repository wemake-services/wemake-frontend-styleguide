{
  "name": "@wemake-services/stylelint-config-scss",
  "version": "0.5.0",
  "description": "Strict shareable config for stylelint and SCSS",
  "main": "index.js",
  "repository": "https://github.com/wemake-services/wemake-frontend-styleguide.git",
  "author": "Nikita Sobolev <mail@sobolevn.me>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "stylelint",
    "stylelint-config",
    "scss",
    "stylelint-scss",
    "standard"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "test": "jest --coverage",
    "coveralls": "npm run test && cat ./.coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "babel": {
    "presets": "env"
  },
  "jest": {
    "preset": "jest-preset-stylelint",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "style-search": "^0.1.0",
    "stylelint": "^9.2.0",
    "stylelint-color-format": "^0.1.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-declaration-strict-value": "^1.0.4",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.1.0"
  }
}
